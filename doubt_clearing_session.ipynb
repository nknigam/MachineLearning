   "source": [
    "def diff(fn, x):\n",
    "    delta = 0.000001\n",
    "    return (fn(x + delta) - fn(x))/ delta\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "def sqcb(t):\n",
    "    x = sq(t)\n",
    "    y = cube(x)\n",
    "    return y"
   ]
